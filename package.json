{
  "name": "RenderTheWorld",
  "version": "1.0.0",
  "description": "积木渲染世界",
  "main": "dist/RenderTheWorld.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint ./src",
    "lint:type": "tsc --noEmit -p ./tsconfig.json",
    "fix": "eslint ./src --fix",
    "start": "tsup && http-server ./dist -p 8080 --cors",
